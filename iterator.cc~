#include "leveldb/db.h"
#include <iostream.h>

using namespace std;

int main(){
  leveldb::DB *db;
  leveldb::Options option;
  leveldb::Iteraor* it=db->NewIterator(leveldb::ReadOptions());
  for(it->SeekToFirst();it->Valid();it->Next()){
    cout<<it->key().ToString()<<": "<<it->value().ToString()<<endl;
  }
  assert(it->status().ok());
  delete it;
}
